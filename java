import java.util.Random;
class NoobProgramming
  public static void main(String [] args)
  {
    Random randy = new Random ();
    int real = 1 + randy.nextInt(3);
    switch(real)
    {
      case 1: System.out.println("Chinntu Sucks"); return;
      case 2: System.out.println("Chinntu has Small PePe"); return;
      case 3: System.out.println("Subscribe to Samuel Miller"); return;
      case 4: System.out.println("Rick ROLLED!!"); return;
      default: System.out.println("Try Again you Noob!");
    }
  }
  //its a small, dont ignoreğŸ™
